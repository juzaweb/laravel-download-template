{
    "name": "juzaweb/laravel-download-template",
    "description": "Download template HTML from URL in Laravel",
    "keywords": [
        "laravel tools",
        "download styles",
        "download templates"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "The Anh Dang",
            "homepage": "https://juzaweb.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "juzaweb/html-dom": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 | 11.0",
        "orchestra/testbench": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Juzaweb\\TemplateDownloader\\": "src/"
        },
        "files": [
            "helpers/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Juzaweb\\TemplateDownloader\\TemplateDownloaderServiceProvider"
            ]
        }
    }
}
